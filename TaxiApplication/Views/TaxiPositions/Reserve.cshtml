@model TaxiApplication.Data.TaxiPosition

@{
    ViewBag.Title = "Reserve";
    Layout = "~/Views/Shared/_Passenger.cshtml";
    var message = TempData["AlertMessage"] ?? string.Empty;
    <link href="/Content/bootstrap.css" rel="stylesheet" />
    <link href="/Content/site.css" rel="stylesheet" />

    <script src="/Scripts/modernizr-2.6.2.js"></script>

    <link href="/Content/tabstyle.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/jq-2.2.3/pdfmake-0.1.18/dt-1.10.12/af-2.1.2/b-1.2.2/b-colvis-1.2.2/b-html5-1.2.2/b-print-1.2.2/fc-3.2.2/fh-3.1.2/kt-2.1.3/r-2.1.0/rr-1.1.2/sc-1.4.2/se-1.2.0/datatables.min.css" />
    <script type="text/javascript" src="https://cdn.datatables.net/v/dt/jq-2.2.3/pdfmake-0.1.18/dt-1.10.12/af-2.1.2/b-1.2.2/b-colvis-1.2.2/b-html5-1.2.2/b-print-1.2.2/fc-3.2.2/fh-3.1.2/kt-2.1.3/r-2.1.0/rr-1.1.2/sc-1.4.2/se-1.2.0/datatables.min.js"></script>

    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />

    <link href="/Content/bootstrap.css" rel="stylesheet" />
    <link href="/Content/Site.css" rel="stylesheet" />
    <link rel="icon" href="/favicon.ico" />
}
<script>
    var msg = '@message';
    if (msg)
        ale(msg);
</script>
<br /><br />
<div class="panel panel-info">
    <div class="panel-heading">
        <h3>Confirm Application</h3>
    </div>
    <div class="panel-body" style="height:25em;overflow-y:auto">
        <h4>Please Confirm Correctness of details.</h4>

        <table class="table table-hover table-striped table-responsive" style="width:100%" id="my_list">
            <thead>

                <tr class="table-header-row">
                    <th>Route Name</th>
                    <td>
                        @Html.DisplayFor(model => model.RouteName)
                    </td>
                </tr>
                <tr>
                    <th>Route Price</th>
                    <td>
                        @Html.DisplayFor(model => model.RoutePrice)
                    </td>
                </tr>
                <tr>
                    <th>Driver Name</th>
                    <td>
                        @Html.DisplayFor(model => model.DriverName)
                    </td>
                </tr>
                <tr>
                    <th>Driver Number</th>
                    <td>
                        @Html.DisplayFor(model => model.DriverTel)
                    </td>
                </tr>
                <tr>
                    <th>Number of Seats</th>
                    <td>
                        @Html.DisplayFor(model => model.NumSeats)
                    </td>
                </tr>
                <tr>
                    <th>Taxi Model</th>
                    <td>
                        @Html.DisplayFor(model => model.TaxiModel)
                    </td>
                </tr>
                <tr>
                    <th>Taxi Make</th>
                    <td>
                        @Html.DisplayFor(model => model.TaxiMake)
                    </td>
                </tr>
                <tr>
                    <th>Taxi Number</th>
                    <td>
                        @Html.DisplayFor(model => model.TaxiNo)
                    </td>
                </tr>
                <tr>
                    <th>Loading Time</th>
                    <td>
                        @Html.DisplayFor(model => model.LoadingTime)
                    </td>
                </tr>
                <tr>
                    <th>Deperture Time</th>
                    <td>
                        @Html.DisplayFor(model => model.DepertureTime)
                    </td>
                </tr>

            </thead>

        </table>
        <script type="text/javascript">
            $('#my_list').dataTable(
                {
                    dom: 'Bfrtip',
                    lengthMenu: [
                        [10, 25, 50, -1],
                        ['10 rows', '25 rows', '50 rows', 'Show all']
                    ],
                    buttons: [
                        /*'pageLength', 'csv',*/ 'pdf', 'print'
                    ],
                    //language: { search: "" },
                    //select: true
                }
            );
            //$(".dataTables_filter input").addClass("search-input");
            //$(".dataTables_filter input").attr('placeholder', 'Search');
            //$('#my_list')
            //    .removeClass('display')
            //    .addClass('table table-striped table-hover');</script>

        <hr />
        <script src="/Scripts/jquery-1.10.2.js"></script>
        <script src="/Scripts/bootstrap.js"></script>
        <script src="/Scripts/respond.js"></script>
        @Html.ActionLink("Cobfirm and Accept payment", "OnceOff", "TaxiPositions", new { area = "" }, new { @class = "btn btn-block btn-success" })
    </div>
</div>








