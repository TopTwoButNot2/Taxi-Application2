@model IEnumerable<TaxiApplication.Data.ReservedList>

@{
    ViewBag.Title = "ViewSeats";
    Layout = "~/Views/Shared/_Passenger.cshtml";
}

<h2>ViewSeats</h2>
 




@using (Html.BeginForm("ViewSeats", "ReservedLists", FormMethod.Get))
{
    @Html.TextBox("search") <input type="submit" value="search" />
}
<tbody>
    @if (Model.Count() == 0)
    {
        <tr>
            <td>
                No Match
            </td>
        </tr>
    }

</tbody>


<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.TaxiNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AvailableSeats)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReservedPassengers)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.TaxiNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AvailableSeats)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReservedPassengers)
            </td>
            <td>
               @Html.ActionLink("BackToList", "ListOfTaxisForPassenger","TaxiPositions")
            </td>
        </tr>
    }

</table>
