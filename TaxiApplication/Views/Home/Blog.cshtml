    <div class="modal fade" id="register" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    @*<button type="button" class="close" data-dismiss="modal">&times;</button>*@
                    <h4 class="modal-title">Passenger Registration</h4>
                </div>

                @using (Html.BeginForm("CreatePassenger", "Home", FormMethod.Post))
                {
                    @Html.AntiForgeryToken()
                    <div class="modal-body">
                        <p>Passenger Details*</p>

                        <label>First Name</label>
                        <input type="text" name="firstname" class="w3-border w3-padding" style="width:100%" required placeholder="Enter First Name" />

                        <label>Surname</label>
                        <input type="text" name="lname" class="w3-border w3-padding" style="width:100%" required placeholder="Enter Surname" />

                        <label>Email</label>
                        <input type="email" name="email" class="w3-border w3-padding" style="width:100%" required placeholder="Enter Email" />

                        <label>Phone Number</label>
                        <input type="number" maxlength="10" name="phone" class="w3-border w3-padding" style="width:100%" required placeholder="Enter 10 digit phone number" />

                        <button class="w3-button" type="submit" style="width:100%;margin-top:5px;background-color: #32BEA6;color:white"> Create Account </button>

                    </div>
                }

                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>

        </div>
