    <div class="modal fade" id="register" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    @*<button type="button" class="close" data-dismiss="modal">&times;</button>*@
                    <h4 class="modal-title">Passenger Registration</h4>
                </div>

                @using (Html.BeginForm("CreatePassenger", "Home", FormMethod.Post))
                {
                    @Html.AntiForgeryToken()
                <div class="modal-body">
                    <p>Passenger Details*</p>

                    <label>First Name</label>
                    <input type="text" name="firstname" class="w3-border w3-padding" style="width:100%" required placeholder="Enter First Name" />

                    <label>Surname</label>
                    <input type="text" name="lname" class="w3-border w3-padding" style="width:100%" required placeholder="Enter Surname" />

                    <label>Email</label>
                    <input type="email" name="email" class="w3-border w3-padding" style="width:100%" required placeholder="Enter Email" />

                    <label>Phone Number</label>
                    <input type="number" maxlength="10" name="phone" class="w3-border w3-padding" style="width:100%" required placeholder="Enter 10 digit phone number" />

                    <label>DepertureDate</label>
                    <input  type="datetime-local" name="DepertureDate" class="w3-border w3-padding" style="width:100%" required placeholder="Enter date" />

                    <button class="w3-button" type="submit" style="width:100%;margin-top:5px;background-color: #32BEA6;color:white"> Create Account </button>

                </div>
                }

                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>

        </div>
    </div>

    <div class="modal fade" id="activate" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header">
                    @*<button type="button" class="close" data-dismiss="modal">&times;</button>*@
                    <h4 class="modal-title">Account Activation</h4>
                </div>

                @using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form", enctype = "multipart/form-data" }))
                {
                    @Html.AntiForgeryToken()
                <div class="modal-body">
                    <p>Account Details*</p>

                    <label>Email</label>
                    <input type="email" name="email" class="w3-border w3-padding" style="width:100%" required placeholder="Enter Email" />

                    <label>Password</label>
                    <input type="password" name="Password" class="w3-border w3-padding" style="width:100%" required placeholder="Create password" />

                    <label>Password Confirmation</label>
                    <input type="password" name="ConfirmPassword" class="w3-border w3-padding" style="width:100%" required placeholder="Confirm password" />
                    
                    <input type="file" name="UserPhoto" id="fileUpload" accept=".png,.jpg,.jpeg,.gif,.tif" />

                    <button class="w3-button" type="submit" style="width:100%;margin-top:5px;background-color: #32BEA6;color:white"> Activate Account </button>

                </div>
                }

                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>

        </div>
    </div>


